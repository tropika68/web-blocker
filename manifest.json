{
  "name": "web blocker",
  "version": "6.0.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "https://www.googleapis.com/",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
